{
  "name": "saswatch",
  "version": "1.0.0",
  "description": "Multi-tenant SaaS platform for tracking Adobe Creative Cloud and Microsoft 365 license usage",
  "main": "server.js",
  "scripts": {
    "start": "node scripts/startup.js",
    "dev": "nodemon server.js",
    "generate-secret": "node generate-secret.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:setup": "powershell -ExecutionPolicy Bypass -File scripts/test-quick-start.ps1",
    "ai:ready": "powershell -ExecutionPolicy Bypass -File scripts/ai-feature-workflow.ps1"
  },
  "dependencies": {
    "@azure/msal-node": "^2.5.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.17.1",
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^10.0.0",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^6.13.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pdfjs-dist": "^4.3.136",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "prisma": "^6.17.1",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "adm-zip": "^0.5.16",
    "nodemon": "^3.0.1",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "microsoft-graph",
    "azure",
    "license-tracking",
    "saas"
  ],
  "author": "RomaTek LLC",
  "license": "AGPL-3.0"
}
